# 环境配置
NODE_ENV=production
PORT=3000
API_PREFIX=/api/v1

# 数据库配置
DB_HOST=your_production_db_host
DB_USER=your_production_db_user
DB_PASSWORD=your_production_db_password
DB_NAME=opswms_prod
DB_POOL_MAX=20
DB_POOL_MIN=5
DB_POOL_ACQUIRE=30000
DB_POOL_IDLE=10000

# JWT配置
JWT_SECRET=your_production_jwt_secret
JWT_EXPIRATION=3600
JWT_REFRESH_EXPIRATION=86400

# Redis配置
REDIS_HOST=your_production_redis_host
REDIS_PORT=6379
REDIS_PASSWORD=your_production_redis_password
REDIS_DB=0
REDIS_KEY_PREFIX=wms_prod:
REDIS_TTL=86400
REDIS_CLUSTER_MODE=false

# 邮件配置
EMAIL_HOST=smtpdm-ap-southeast-1.aliyun.com
EMAIL_PORT=465
EMAIL_SECURE=true
EMAIL_USER=ops@opsaway.cc
EMAIL_PASSWORD=your_production_email_password
EMAIL_RETRY_COUNT=3
EMAIL_RETRY_DELAY=1000
EMAIL_SENDER_NAME=OPS-WMS
EMAIL_TEST_MODE=false
EMAIL_REDIRECT_ALL_TO=ops@opsaway.cc

# 前端配置
FRONTEND_URL=https://wms.opsaway.cc

# CORS配置
CORS_ORIGIN=https://wms.opsaway.cc
CORS_CREDENTIALS=true

# 管理员配置
ADMIN_EMAIL=admin@opsaway.cc
ADMIN_PASSWORD=your_production_admin_password
ADMIN_ROLE=admin 